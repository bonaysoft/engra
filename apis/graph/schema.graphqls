# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Vocabulary {
  id: ID!
  text: String!
  done: Boolean!
  user: User!
}

type User {
  id: ID!
  name: String!
}

type Query {
  vocabularies(tag: String!): [Vocabulary!]!
}

input NewTodo {
  text: String!
  userId: String!
}

type Mutation {
  addVocabulary(input: NewTodo!): Vocabulary!
}
